                    </div>
                    <!-- ==================================== CURRENCY INSIDE NAVBAR -->
                    <div class="main-panel-currency">
                        <h4>Günlük məzənnə</h4>
                        <div class="panel-currency-line">
                            <div class="panel-currency-flag">
                                <img src="~/PanelPage/images/flg-az.png" alt="Flag Here!">
                                <span>1</span>
                            </div>
                            <span>AZN</span>
                        </div>
                        <div class="panel-currency-line">
                            <div class="panel-currency-flag">
                                <img src="~/PanelPage/images/flg-tr.png" alt="Flag Here!">
                                @{
                                    for (int i = 0; i < @Model.rootobject.ValCurs.ValType[1].Valute.Length; i++)
                                    {
                                        if (@Model.rootobject.ValCurs.ValType[1].Valute[i].Name == "1 Türkiyə lirəsi")
                                        {
                                            var tlAmount = 1 / decimal.Parse(@Model.rootobject.ValCurs.ValType[1].Valute[i].Value);
                                            var truncatedTl = Math.Truncate(1000 * tlAmount) / 1000;
                                            <span>@truncatedTl </span>
                                        }
                                    }
                                }
                            </div>
                            <span>TL</span>
                        </div>
                        <div class="panel-currency-line">
                            <div class="panel-currency-flag">
                                <img src="~/PanelPage/images/flg-usa.png" alt="Flag Here!">
                                @{
                                    for (int i = 0; i < @Model.rootobject.ValCurs.ValType[1].Valute.Length; i++)
                                    {
                                        if (@Model.rootobject.ValCurs.ValType[1].Valute[i].Name == "1 ABŞ dolları")
                                        {
                                            var usAmount = 1 / decimal.Parse(@Model.rootobject.ValCurs.ValType[1].Valute[i].Value);
                                            var truncatedUSD = Math.Truncate(100 * usAmount) / 100;
                                            <span>@truncatedUSD </span>
                                        }
                                    }
                                }
                            </div>
                            <span>USD</span>
                        </div>
                    </div>
