                                <select name="Input" id="Input">
                                    <option value="1">AZN</option>
                                    <option value="2">USD</option>
                                    <option value="3">TRY</option>
                                </select>
                            </div>
                            <div class="calculator-output">
                                <input type="number" placeholder="0.0" disabled>
                                <select name="Output" id="Output">
                                    <option value="1">AZN</option>
                                    <option value="2">USD</option>
                                    <option value="3">TRY</option>
                                </select>
                            </div>
                        </form>
