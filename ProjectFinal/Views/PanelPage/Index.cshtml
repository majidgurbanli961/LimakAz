                        </div>
                        <!-- ==================================== TABLE -->
                        <div class="hidden-panel-page-bottom">
                            <div class="hidden-panel-page-bottom-header">
                                <div>
                                    <p>Filter</p>
                                    <select name="Filter" id="Filter">
                                        <option value="1">Hamisi</option>
                                        <option value="2">Məxaric</option>
                                        <option value="3">Mədaxil</option>
                                    </select>
                                </div>
                                <a asp-controller="PanelPage" asp-action="DownloadExcell" class="transparent text-secondary">
                                    <img src="~/PanelPage/images/excel.png" alt="excel">
                                    <span>Excel faylda yaddaşa ver</span>
                                </a>
                            </div>
                            <div class="hidden-panel-page-bottom-table">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>Əməliyyat</th>
                                            <th>Məbləğ</th>
                                            <th>Tarix</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{
                                            var transactions = Model.Transactions.Where(x => x.CurrencyType == 1).ToList();
                                            if (transactions.Count() > 0)
                                            {
                                                foreach (var transaction in transactions)
                                                {
                                                    <tr>
                                                        @{
                                                            if (transaction.TransactionType == 1)
                                                            {
                                                                <td>Mədaxil</td>
                                                            }
                                                            else if (transaction.TransactionType == 2)
                                                            {
                                                                <td>Məxaric</td>
                                                            }
                                                        }
                                                        <td>@transaction.Amount</td>
                                                        <td>@transaction.Date.ToString("dd/MM/yyyy")</td>
                                                    </tr>
                                                }
                                            }
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
