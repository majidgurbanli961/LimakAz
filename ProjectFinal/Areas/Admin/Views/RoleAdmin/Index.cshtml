@model List<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Index";
}

@*<a class="btn btn-outline-success mt-4" href="/Admin/RoleAdmin/CreateRole">Yeni Rol Yarat</a>*@
<br />
<br />

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Role adi</th>
            
        </tr>
    </thead>
    <tbody>
        @{
            for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>
                        @Model[i].Id
                    </td>
                    <td>
                        @Model[i].Name
                    </td>
                 


                </tr>

            }
        }


    </tbody>
</table>

