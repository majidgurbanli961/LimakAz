                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text">   Vergi numarası</div>
                    </div>
                    <input asp-for="TaxNumber" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="TaxNumber" class="text-danger"></span>
                </div>
            </div>
          
            <div class="form-group w-100 text-center">
                <input style="width:150px" type="submit" value="Düzəliş Edin" class="text-center btn btn-outline-success mt-3" />
            </div>



