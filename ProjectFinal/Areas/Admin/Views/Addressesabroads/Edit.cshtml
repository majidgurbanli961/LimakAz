                    <div class="input-group-prepend">
                        <div class="input-group-text">Şəhər</div>
                    </div>
                    <input asp-for="City" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="City" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text">İlçe</div>
                    </div>
                    <input asp-for="District" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="District" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Vergi dairesi</div>
                    </div>
                    <input asp-for="TaxAdministration" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="TaxAdministration" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text">Semt</div>
                    </div>
                    <input asp-for="DistrictTwo" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="DistrictTwo" class="text-danger"></span>
                </div>
            </div>


            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text"> ZIP/Post kodu</div>
                    </div>
                    <input asp-for="ZIP" class="form-control" id="inlineFormInputGroupUsername2" />
                    <span asp-validation-for="ZIP" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text"> Status</div>
                    </div>

                    <select asp-for="@Model.CountryId" class="form-control">
                        <option value="1">Türkiyə</option>
                        <option value="2">Amerika </option>


                    </select>
                    <span asp-validation-for="CountryId" class="text-danger"></span>
                </div>
            </div>

            <div class="form-group">

                <label class="sr-only" for="inlineFormInputGroupUsername2">Username</label>
                <div class="input-group mb-2 mr-sm-2">
                    <div class="input-group-prepend">
                        <div class="input-group-text"> TC kimlik</div>
                    </div>
                    <input asp-for="identityNumber" class="form-control" id="inlineFormInputGroupUsername2" />
